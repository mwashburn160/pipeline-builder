    "docker:tag": "docker image tag ${PROJECT_NAME:-plugin}:$(jq -r .version package.json) ${REGISTRY:-ghcr.io/mwashburn160}/${PROJECT_NAME:-plugin}:$(jq -r .version package.json)",
